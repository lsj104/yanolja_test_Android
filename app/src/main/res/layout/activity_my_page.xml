<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".src.main.myPage.MyPageActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="50dp">

        <TextView
            android:id="@+id/my_text_ya"
            android:layout_width="280dp"
            android:layout_height="match_parent"
            android:text="@string/my_text_ya"
            android:layout_marginStart="20dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:gravity="center_vertical"
            android:textSize="16sp"

            app:layout_constraintStart_toStartOf="parent"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:padding="6dp"
            android:id="@+id/my_btn_alarm"
            android:layout_marginStart="10dp"
            app:layout_constraintStart_toEndOf="@+id/my_text_ya"
            android:layout_width="30dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toStartOf="@id/my_btn_cart"
            android:src="@drawable/myalarm" />

        <androidx.appcompat.widget.AppCompatImageView
            app:layout_constraintStart_toEndOf="@id/my_btn_alarm"
            android:id="@+id/my_btn_cart"
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:src="@drawable/cart"
            app:layout_constraintEnd_toEndOf="parent"
            android:padding="5dp"/>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/my_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_width="match_parent"
                android:layout_height="290dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="1.5dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/my_layout_login"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="?attr/selectableItemBackground">

                        <TextView
                            android:layout_marginTop="15dp"
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_text_signup"
                            android:layout_width="200dp"
                            android:layout_height="14dp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:text="@string/my_text_signup"
                            android:textSize="10sp"
                            android:gravity="center_vertical" />

                        <ImageView
                            android:layout_marginTop="3dp"
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_btn_signup"
                            android:layout_width="200dp"
                            android:layout_height="30dp"
                            app:layout_constraintTop_toBottomOf="@id/my_text_signup"
                            app:layout_constraintStart_toStartOf="parent"
                            android:src="@drawable/mysignup"
                            android:scaleType="fitStart"/>

                        <ImageView
                            android:layout_width="100dp"
                            android:layout_height="30dp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:src="@drawable/myblue"
                            android:scaleType="fitCenter"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/my_btn_signup"
                            android:layout_marginTop="5dp"
                            android:layout_marginStart="45dp"
                            android:layout_marginBottom="5dp"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="#f2f2f2"
                            android:layout_marginTop="10dp"
                            app:layout_constraintTop_toBottomOf="@id/my_btn_signup"
                            app:layout_constraintStart_toStartOf="parent"/>


                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        >

                        <ImageView
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_img_point"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="15dp"
                            android:src="@drawable/mypoint"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_gravity="center"/>

                        <TextView
                            android:id="@+id/my_text_point"
                            app:layout_constraintStart_toEndOf="@id/my_img_point"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_width="60dp"
                            android:layout_height="20dp"
                            android:textColor="@color/black"
                            android:layout_marginBottom="15dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="10dp"
                            android:textSize="13sp"
                            android:text="@string/my_text_point"/>

                        <ImageView
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="15dp"
                            android:scaleType="fitCenter"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="#f2f2f2"
                            android:layout_marginTop="16dp"
                            app:layout_constraintTop_toBottomOf="@id/my_text_point"
                            app:layout_constraintStart_toStartOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        >

                        <ImageView
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_img_coin"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="15dp"
                            android:src="@drawable/mycoin"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginTop="10dp"
                            android:layout_gravity="center"/>

                        <TextView
                            android:id="@+id/my_text_coin"
                            android:layout_marginTop="10dp"
                            app:layout_constraintStart_toEndOf="@id/my_img_coin"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_width="100dp"
                            android:layout_height="20dp"
                            android:textColor="@color/black"
                            android:layout_marginBottom="15dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="10dp"
                            android:textSize="13sp"
                            android:text="@string/my_text_coin"/>

                        <ImageView
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="15dp"
                            android:layout_marginTop="10dp"
                            android:scaleType="fitCenter"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="#f2f2f2"
                            android:layout_marginTop="16dp"
                            app:layout_constraintTop_toBottomOf="@id/my_text_coin"
                            app:layout_constraintStart_toStartOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        >

                        <ImageView
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_img_coupon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="15dp"
                            android:src="@drawable/mycoupon"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="10dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_gravity="center"/>

                        <TextView
                            android:id="@+id/my_text_coupon"
                            app:layout_constraintStart_toEndOf="@id/my_img_coupon"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_width="60dp"
                            android:layout_height="20dp"
                            android:textColor="@color/black"
                            android:layout_marginBottom="15dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            android:layout_marginTop="10dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="10dp"
                            android:textSize="13sp"
                            android:text="@string/my_text_coupon"/>

                        <ImageView
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="15dp"
                            android:layout_marginTop="10dp"
                            android:scaleType="fitCenter"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="#f2f2f2"
                            android:layout_marginTop="16dp"
                            app:layout_constraintTop_toBottomOf="@id/my_text_coupon"
                            app:layout_constraintStart_toStartOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal"
                        >

                        <ImageView
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_img_review"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="15dp"
                            android:src="@drawable/myreview"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="10dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_gravity="center"/>

                        <TextView
                            android:id="@+id/my_text_review"
                            app:layout_constraintStart_toEndOf="@id/my_img_review"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_width="60dp"
                            android:layout_height="20dp"
                            android:textColor="@color/black"
                            android:layout_marginBottom="15dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:textSize="13sp"
                            android:text="@string/my_text_review"/>

                        <ImageView
                            android:id="@+id/my_btn_review"
                            app:layout_constraintStart_toEndOf="@+id/my_text_review"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="15dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginStart="45dp"
                            android:scaleType="fitCenter"/>


                        <View
                            android:id="@+id/my_view_review"
                            android:layout_width="1dp"
                            android:layout_height="50dp"
                            android:background="#f2f2f2"
                            android:layout_marginTop="10dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="20dp"
                            app:layout_constraintStart_toEndOf="@id/my_btn_review"/>

                        <ImageView
                            android:layout_marginStart="20dp"
                            android:id="@+id/my_img_save"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginBottom="15dp"
                            android:src="@drawable/mysave"
                            app:layout_constraintStart_toEndOf="@id/my_view_review"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_gravity="center"/>

                        <TextView
                            android:id="@+id/my_text_save"
                            app:layout_constraintStart_toEndOf="@id/my_img_save"
                            android:layout_width="60dp"
                            android:layout_height="20dp"
                            android:textColor="@color/black"
                            android:layout_marginBottom="15dp"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginStart="10dp"
                            android:textSize="13sp"
                            android:text="@string/my_text_save"/>

                        <ImageView
                            app:layout_constraintEnd_toEndOf="parent"
                            android:id="@+id/my_btn_save"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_marginBottom="20dp"
                            android:layout_marginEnd="20dp"
                            android:scaleType="fitCenter"/>


                    </androidx.constraintlayout.widget.ConstraintLayout>



                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_marginTop="20dp"
                android:layout_height="68dp"
                android:src="@drawable/mynotice"
                android:scaleType="fitCenter"/>

            <TextView
                android:id="@+id/my_text_unsign"
                android:layout_width="match_parent"
                android:layout_height="18dp"
                android:textSize="13sp"
                android:text="@string/my_text_unsign"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:textColor="@color/black"
                android:textStyle="bold"/>

            <androidx.cardview.widget.CardView
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="1.5dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/my_text_domestic_all"
                        android:layout_width="300dp"
                        android:layout_height="match_parent"
                        android:text="@string/my_text_domestic_all"
                        android:gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textSize="13sp"
                        android:layout_marginStart="20dp"/>

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/mymore"
                        android:layout_gravity="center"
                        android:layout_marginStart="20dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_width="match_parent"
                android:layout_height="205dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="1.5dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/my_text_domestic_hotel"
                                android:layout_width="300dp"
                                android:layout_height="match_parent"
                                android:text="@string/my_text_domestic_hotel"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/black"
                                android:textSize="13sp"
                                android:layout_marginStart="20dp"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:src="@drawable/mymore"
                                android:layout_gravity="center"
                                android:layout_marginStart="20dp"/>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_marginTop="5dp"
                            android:layout_height="2px"
                            android:background="#f2f2f2" />




                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/my_text_domestic_mobile"
                                android:layout_width="300dp"
                                android:layout_height="match_parent"
                                android:text="@string/my_text_domestic_mobile"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/black"
                                android:textSize="13sp"
                                android:layout_marginStart="20dp"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:src="@drawable/mymore"
                                android:layout_gravity="center"
                                android:layout_marginStart="20dp"/>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_marginTop="5dp"
                            android:layout_height="2px"
                            android:background="#f2f2f2" />




                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/my_text_domestic_ktx"
                                android:layout_width="300dp"
                                android:layout_height="match_parent"
                                android:text="@string/my_text_domestic_ktx"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/black"
                                android:textSize="13sp"
                                android:layout_marginStart="20dp"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:src="@drawable/mymore"
                                android:layout_gravity="center"
                                android:layout_marginStart="20dp"/>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_marginTop="5dp"
                            android:layout_height="2px"
                            android:background="#f2f2f2" />




                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/my_text_domestic_food"
                                android:layout_width="300dp"
                                android:layout_height="match_parent"
                                android:text="@string/my_text_domestic_food"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/black"
                                android:textSize="13sp"
                                android:layout_marginStart="20dp"/>

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:src="@drawable/mymore"
                                android:layout_gravity="center"
                                android:layout_marginStart="20dp"/>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="1.5dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/my_text_abroad_all"
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="5dp"
                            android:text="@string/my_text_abroad_all"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:layout_marginStart="20dp"/>

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_gravity="center"
                            android:layout_marginStart="20dp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_marginTop="5dp"
                        android:layout_height="2px"
                        android:background="#f2f2f2" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/my_text_abroad_insurance"
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:text="@string/my_text_abroad_insurance"
                            android:gravity="center_vertical"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:layout_marginStart="20dp"/>

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/mymore"
                            android:layout_gravity="center"
                            android:layout_marginStart="20dp"/>

                    </LinearLayout>

                </LinearLayout>


            </androidx.cardview.widget.CardView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:src="@drawable/mynotice2"
                android:layout_marginTop="10dp"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/story1"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/myevent"/>

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/my_text_event"
                        android:layout_width="100dp"
                        android:layout_height="20dp"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:textSize="13sp"
                        android:layout_marginBottom="20dp"
                        android:text="@string/my_text_event"/>
                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#f2f2f2" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
